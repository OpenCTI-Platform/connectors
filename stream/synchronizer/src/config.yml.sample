opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'STREAM'
  name: 'Synchronizer'
  scope: 'synchronizer'
  confidence_level: 15 # From 0 (Unknown) to 100 (Fully trusted)
  log_level: 'info'

remote_opencti:
  url: 'https://ChangeMe.com' # Remote OpenCTI URL
  live_stream_id: 'live' # ID of the live stream created in the OpenCTI UI
  ssl_verify: true
  token: 'ChangeMe'
  start_timestamp: '' # Optional, start to synchronize from a specific date