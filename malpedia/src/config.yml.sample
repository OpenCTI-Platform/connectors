opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'EXTERNAL_IMPORT'
  name: 'Malpedia'
  scope: 'malpedia'
  confidence_level: 15 # From 0 (Unknown) to 100 (Fully trusted)
  update_existing_data: false
  log_level: 'info'

malpedia:
  auth_key: ''
  interval_sec: 86400
  import_intrusion_sets: false
  import_yara: true
  create_indicators: true # Required, create indicators for hashes
  create_observables: true # Required, create observables for hashes
