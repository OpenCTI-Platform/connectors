opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'EXTERNAL_IMPORT'
  name: 'Cyber Monitor Github'
  scope: 'report'
  confidence_level: 0 # From 0 (Unknown) to 100 (Fully trusted)
  update_existing_data: false
  run_and_terminate: false
  log_level: 'info'

cyber_monitor:
  github_token: '' # If not provided, rate limit will be very low
  from_year: '2018'
  interval: 4 # In days, must be strictly greater than 1
