opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'EXTERNAL_IMPORT'
  name: 'Citalid'
  scope: 'citalid'
  confidence_level: 30 # From 0 (Unknown) to 100 (Fully trusted)
  update_existing_data: true
  run_and_terminate: false
  log_level: 'info'

citalid:
  customer_sub_domain_url: 'https://ChangeMe.citalid.com'
  user: 'ChangeMe'
  password: 'ChangeMe'
  interval: 24 # In hours
