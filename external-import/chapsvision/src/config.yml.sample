opencti:
  url: 'http://localhost'
  token: 'ChangeMe'

connector:
  type: 'EXTERNAL_IMPORT'
  id: 'ChangeMe'
  name: 'Chapsvision'
  scope: 'chapsvision'
  run_and_terminate: false
  log_level: 'info'

chapsvision:
  certificate: 'certificate in base64'
  key: 'key in base64'
  query_parameter: '_query_param'
  start_date: '2022-01-01'
  interval: 5 # In minutes
