opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'EXTERNAL_IMPORT'
  name: 'MITRE Datasets'
  scope: 'tool,report,malware,identity,campaign,intrusion-set,attack-pattern,course-of-action,x-mitre-data-source,x-mitre-data-component,x-mitre-matrix,x-mitre-tactic,x-mitre-collection'
  run_and_terminate: false
  log_level: 'info'
  expose_metrics: False

mitre:
  remove_statement_marking: true
  interval: 7 # In days
