{
	"name": "MISP",
	"description": "Import data from a MISP instance.",
	"fields": {
		"name": {
			"type": "text",
			"description": "Name of this MISP instance",
			"default": "MISP"
		},
		"url": {
			"type": "text",
			"description": "URL of the MISP instance",
			"default": ""
		},
		"key": {
			"type": "text",
			"description": "API Key",
			"default": ""
		},
		"tag": {
			"type": "text",
			"description": "Tag for events that have to be imported",
			"default": "OpenCTI: Import"
		},
		"untag_event": {
			"type": "switch",
			"description": "Remove the tag given above from the event",
			"default": true
		},
		"imported_tag": {
			"type": "text",
			"description": "Tag to be set after successful import of MISP event",
			"default": "OpenCTI: Imported"
		},
		"tag_event": {
			"type": "switch",
			"description": "Enable tagging of successfully processed MISP events with the above set tag.",
			"default": true
		},
		"cron": {
			"type": "select",
			"multiple": false,
			"description": "Synchronization scheduling",
			"default": "realtime",
			"options": [
				{
					"label": "Real time (5 min)",
					"key": "realtime"
				}
			]
		},
		"active": {
			"type": "switch",
			"description": "Enable synchronization",
			"default": false
		}
	}
}