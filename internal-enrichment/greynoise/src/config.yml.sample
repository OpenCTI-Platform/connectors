opencti:
  url: 'http://localhost'
  token: 'ChangeMe'

connector:
  type: 'INTERNAL_ENRICHMENT'
  id: 'ChangeMe'
  name: 'GreyNoise IP Enrichment'
  scope: 'IPv4-Addr'
  auto: true # Enable/disable auto-enrichment of observables
  log_level: 'error'

greynoise:
  key: 'ChangeMe'
  max_tlp: 'TLP:AMBER'
  sighting_not_seen: false
  no_sightings: false