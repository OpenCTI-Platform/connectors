opencti:
  url: 'http://localhost:8080'
  token: 'ChangeMe'

connector:
  id: 'ChangeMe'
  type: 'INTERNAL_ENRICHMENT'
  name: 'Shodan'
  scope: 'IPv4-Addr'
  auto: true # Enable/disable auto-enrichment of observables
  log_level: 'info'

shodan:
  token: 'ChangeMe'
  max_tlp: 'TLP:AMBER'
  create_indicators: false
  default_score: 50 # Former confidence_level deprecated in OpenCTI version >= 6.0
